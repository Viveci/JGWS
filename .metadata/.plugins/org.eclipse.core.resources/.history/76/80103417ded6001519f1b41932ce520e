package utillity.math;

public class MyMath{

   public static int factorial(int n){
      return 1 + sum(n-1);
   }
   
   public static int sum(int n){
      if(n <= 0){
         return 0;
      }
      else{
         return sum(n-1);
      }
   }
   
   public static void main(String[] args){
      System.out.println(factorial(4));
   }
   
   
}
